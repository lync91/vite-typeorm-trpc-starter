{
  "name": "react-vite-express-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev-node": "tsx watch src/server",
    "start": "node dist/server/",
    "build": "tsc && vite build && tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/db/data-source.ts migration:generate ./src/db/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/db/data-source.ts migration:run"
  },
  "dependencies": {
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "remult": "^0.22.7",
    "trpc-playground": "^1.0.4",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
